{
  "manifest_version": 3,
  "name": "Threespector",
  "description": "Collection of tools to inspect threejs applications.",
  "version": "0.1.0",
  "devtools_page": "index.html",
  "author": "hello@jonasklein.dev",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "serviceWorker.js",
    "type": "module"
  }
}
